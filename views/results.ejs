
<!--Include Header-->
    <%- include('include/_header') %>
<!--/Include Header-->

<!--Main-->
<main id="site-main">
    <div class="container">
            <br>
            <br>
            <br>
        <div class="box-nav d-flex justify-between">
            
            <a href="/add-user" class="border-shadow">
                <span class="text-gradient">New User <i class="fas fa-user"></i></span>
            </a>
        </div>
        <!--Form Handling-->
        <input type="text" id="myInput" onkeyup="myFunction()" placeholder="Search for names.." title="Type in a name">


        <form action="/" method="POST">
            <table class="table">
                <thead class="thead-dark">
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>IC Number</th>
                        <th>IC Colour</th>
                        <th>Gender</th>
                        <th>Nationality</th>
                        <th>Date of Birth</th>
                        <th>Grade</th>
                        <th>School</th>
                        <th>Question 1</th>
                        <th>Question 2</th>
                        <th>Question 3</th>
                        <th>Edit</th>
                        <th>Delete</th>
                    </tr>
                </thead>
                <tbody>
                    <%- include('include/_show') %>
                </tbody>
            </table>
        </form>
    </div>
</main>
<!--/Main-->
<!--Include Footer-->
    <%- include('include/_footer') %>
<!--/Include Footerer-->